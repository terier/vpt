{
  "type": "sidebar",
  "bind": "sidebar",
  "children": [
    {
      "type": "panel",
      "scrollable": true,
      "children": [
        {
          "type": "tabs",
          "children": {
            "Data": {
              "type": "panel",
              "children": [
                {
                  "type": "accordion",
                  "bind": "volumeLoadContainer",
                  "label": "Volume"
                },
                {
                  "type": "accordion",
                  "bind": "envmapLoadContainer",
                  "label": "Environment"
                }
              ]
            },
            "Settings": {
              "type": "panel",
              "children": [
                {
                  "type": "accordion",
                  "label": "Renderer",
                  "children": [
                    {
                      "type": "field",
                      "label": "Renderer",
                      "children": [
                        {
                          "type": "dropdown",
                          "bind": "rendererSelect",
                          "options": [
                            {
                              "value": "mip",
                              "label": "Maximum intensity projection"
                            },
                            {
                              "value": "iso",
                              "label": "Isosurface extraction"
                            },
                            {
                              "value": "eam",
                              "label": "Emission-absorption model"
                            },
                            {
                              "value": "dos",
                              "label": "Directional occlusion shading"
                            },
                            {
                              "value": "mcs",
                              "label": "Single scattering"
                            },
                            {
                              "selected": true,
                              "value": "mcm",
                              "label": "Multiple scattering"
                            },
                            {
                              "value": "fld",
                              "label": "Flux-Limited Diffusion"
                            },
                            {
                              "value": "dif",
                              "label": "Flux-unlimited Diffusion"
                            },
                            {
                              "value": "fdo",
                              "label": "Flux-Limited Diffusion (Original)"
                            },
                            {
                              "value": "fdm",
                              "label": "Flux-Limited Diffusion (Multigrid)"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "panel",
                      "bind": "rendererSettingsContainer"
                    }
                  ]
                },
                {
                  "type": "accordion",
                  "label": "Tone mapper",
                  "children": [
                    {
                      "type": "field",
                      "label": "Tone mapper",
                      "children": [
                        {
                          "type": "dropdown",
                          "bind": "toneMapperSelect",
                          "options": [
                            {
                              "value": "artistic",
                              "label": "Artistic",
                              "selected": true
                            },
                            {
                              "value": "range",
                              "label": "Range"
                            },
                            {
                              "value": "reinhard",
                              "label": "Reinhard"
                            },
                            {
                              "value": "reinhard2",
                              "label": "Reinhard 2"
                            },
                            {
                              "value": "uncharted2",
                              "label": "Uncharted 2"
                            },
                            {
                              "value": "filmic",
                              "label": "Filmic"
                            },
                            {
                              "value": "unreal",
                              "label": "Unreal"
                            },
                            {
                              "value": "aces",
                              "label": "Aces"
                            },
                            {
                              "value": "lottes",
                              "label": "Lottes"
                            },
                            {
                              "value": "uchimura",
                              "label": "Uchimura"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "panel",
                      "bind": "toneMapperSettingsContainer"
                    }
                  ]
                },
                {
                  "type": "accordion",
                  "bind": "renderingContextSettingsContainer",
                  "label": "Context"
                }
              ]
            },
            "Evaluation": {
              "type": "panel",
              "children": [
                {
                  "type": "accordion",
                  "label": "MVP",
                  "children": [
                    {
                      "type": "field",
                      "label": "Set MVP:",
                      "children": [
                        {
                          "type": "textbox",
                          "bind": "setMVP",
                          "value": ""
                        }
                      ]
                    },
                    {
                      "type": "field",
                      "children": [
                        {
                          "type": "button",
                          "bind": "getMVP",
                          "label": "Print MVP"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "accordion",
                  "label": "Iteration Testing",
                  "children": [
                    {
                      "type": "field",
                      "label": "Total Iterations:",
                      "children": [
                        {
                          "type": "spinner",
                          "bind": "totalIterations",
                          "value": 100,
                          "min": 0
                        }
                      ]
                    },
                    {
                      "type": "field",
                      "label": "Intervals:",
                      "children": [
                        {
                          "type": "spinner",
                          "bind": "intervals",
                          "value": 100,
                          "min": 0
                        }
                      ]
                    },
                    {
                      "type": "field",
                      "label": "Save As:",
                      "children": [
                        {
                          "type": "textbox",
                          "bind": "saveAs",
                          "value": "FDO"
                        }
                      ]
                    },
                    {
                      "type": "field",
                      "children": [
                        {
                          "type": "button",
                          "bind": "startIterationTesting",
                          "label": "Start Iteration Testing"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "accordion",
                  "label": "Start Sequence",
                  "children": [
                    {
                      "type": "field",
                      "label": "Testing Time:",
                      "children": [
                        {
                          "type": "spinner",
                          "bind": "sequenceTestingTime",
                          "value": 30,
                          "min": 0
                        }
                      ]
                    },
                    {
                      "type": "field",
                      "label": "Intervals:",
                      "children": [
                        {
                          "type": "spinner",
                          "bind": "sequenceIntervals",
                          "value": 1000,
                          "min": 0
                        }
                      ]
                    },
                    {
                      "type": "field",
                      "label": "Save As:",
                      "children": [
                        {
                          "type": "textbox",
                          "bind": "sequenceSaveAs",
                          "value": "FDO"
                        }
                      ]
                    },
                    {
                      "type": "field",
                      "label": "Starting Scale:",
                      "children": [
                        {
                          "type": "vector",
                          "bind": "sequenceStartingScale",
                          "value": { "x": 1, "y": 1, "z": 1 }
                        }
                      ]
                    },
                    {
                      "type": "field",
                      "label": "Starting Rotation:",
                      "children": [
                        {
                          "type": "vector",
                          "bind": "sequenceStartingRotation",
                          "value": { "x": 0, "y": 0, "z": 0 }
                        }
                      ]
                    },
                    {
                      "type": "field",
                      "label": "Starting Translation:",
                      "children": [
                        {
                          "type": "vector",
                          "bind": "sequenceStartingTranslation",
                          "value": { "x": 0, "y": 0, "z": 0 }
                        }
                      ]
                    },
                    {
                      "type": "field",
                      "label": "Start Camera Zoom:",
                      "children": [
                        {
                          "type": "spinner",
                          "bind": "sequenceStartCameraZoom",
                          "value": 0,
                          "min": 0
                        }
                      ]
                    },
                    {
                      "type": "field",
                      "label": "Sequence:",
                      "children": [
                        {
                          "type": "textbox",
                          "bind": "sequenceSequence",
                          "value": ""
                        }
                      ]
                    },
                    {
                      "type": "field",
                      "children": [
                        {
                          "type": "button",
                          "bind": "startSequenceTesting",
                          "label": "Start Sequence Testing"
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            "About": {
              "type": "panel",
              "bind": "about"
            }
          }
        }
      ]
    }
  ]
}