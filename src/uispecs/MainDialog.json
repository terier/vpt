{
  "type": "sidebar",
  "bind": "sidebar",
  "children": [
    {
      "type": "panel",
      "scrollable": true,
      "children": [
        {
          "type": "tabs",
          "children": {
            "Data": {
              "type": "panel",
              "children": [
                {
                  "type": "accordion",
                  "bind": "volumeLoadContainer",
                  "label": "Volume"
                },
                {
                  "type": "accordion",
                  "bind": "envmapLoadContainer",
                  "label": "Environment"
                }
              ]
            },
            "Settings": {
              "type": "panel",
              "children": [
                {
                  "type": "accordion",
                  "label": "Renderer",
                  "children": [
                    {
                      "type": "field",
                      "label": "Renderer",
                      "children": [
                        {
                          "type": "dropdown",
                          "bind": "rendererSelect",
                          "options": [
                            {
                              "value": "mip",
                              "label": "Maximum intensity projection"
                            },
                            {
                              "value": "iso",
                              "label": "Isosurface extraction"
                            },
                            {
                              "value": "eam",
                              "label": "Emission-absorption model"
                            },
                            {
                              "value": "dos",
                              "label": "Directional occlusion shading"
                            },
                            {
                              "value": "mcs",
                              "label": "Single scattering"
                            },
                            {
                              "value": "mcm",
                              "label": "Multiple scattering"
                            },
                            {
                              "selected": true,
                              "value": "fld",
                              "label": "Flux-Limited Diffusion"
                            },
                            {
                              "selected": true,
                              "value": "fdo",
                              "label": "Flux-Limited Diffusion (Original)"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "panel",
                      "bind": "rendererSettingsContainer"
                    }
                  ]
                },
                {
                  "type": "accordion",
                  "label": "Tone mapper",
                  "children": [
                    {
                      "type": "field",
                      "label": "Tone mapper",
                      "children": [
                        {
                          "type": "dropdown",
                          "bind": "toneMapperSelect",
                          "options": [
                            {
                              "value": "artistic",
                              "label": "Artistic",
                              "selected": true
                            },
                            {
                              "value": "range",
                              "label": "Range"
                            },
                            {
                              "value": "reinhard",
                              "label": "Reinhard"
                            },
                            {
                              "value": "reinhard2",
                              "label": "Reinhard 2"
                            },
                            {
                              "value": "uncharted2",
                              "label": "Uncharted 2"
                            },
                            {
                              "value": "filmic",
                              "label": "Filmic"
                            },
                            {
                              "value": "unreal",
                              "label": "Unreal"
                            },
                            {
                              "value": "aces",
                              "label": "Aces"
                            },
                            {
                              "value": "lottes",
                              "label": "Lottes"
                            },
                            {
                              "value": "uchimura",
                              "label": "Uchimura"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "panel",
                      "bind": "toneMapperSettingsContainer"
                    }
                  ]
                },
                {
                  "type": "accordion",
                  "bind": "renderingContextSettingsContainer",
                  "label": "Context"
                }
              ]
            },
            "About": {
              "type": "panel",
              "bind": "about"
            }
          }
        }
      ]
    }
  ]
}